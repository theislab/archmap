{
  "name": "archmap_backend",
  "version": "1.0.0",
  "description": "Archmap Backend",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "test": "jest",
    "start": "node ./build/server.js",
    "gcp-build": "npm run build",
    "dev:start": "npm-run-all build start",
    "dev": "tsc-watch --onSuccess \"npm run start\"",
    "build": "npm-run-all build:ts build:emails",
    "build:ts": "tsc -p .",
    "build:emails:createtailwind": "cd scripts && npx tailwindcss -c ./tailwind.config.js -i ./base.css -o build/tailwind.css",
    "build:emails:compile": "node scripts/compileEmails.cjs",
    "build:emails": "npm-run-all build:emails:compile",
    "full-backend-local:docker": "docker-compose -f dev/docker-compose.yml up -d",
    "full-backend-local": "npm-run-all full-backend-local:docker build:emails dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theislab/archmap.git"
  },

{
  "extends": "@tsconfig/node14/tsconfig.json",
    "compilerOptions": {
        "outDir": "./build/",
        "allowJs": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "strict": false,
        "target": "ES2020"
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
      "./src/*",
      "./src/**/*"
    ]
}
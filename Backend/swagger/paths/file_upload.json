{
  "/file_upload/complete_upload": {
    "post": {
      "summary": "Complete upload",
      "tags": ["Project"],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "parts": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "ETag": {
                        "type": "string"
                      },
                      "PartNumber": {
                        "type": "number"
                      }
                    }
                  }
                },
                "uploadId": {
                  "type": "string"
                }
              },
              "required": ["parts", "uploadId"],
              "additionalProperties": false
            }
          }
        }
      }
    }
  },
  "/file_upload/get_upload_url": {
    "get": {
      "summary": "Get upload URL",
      "tags": ["Project"],
      "parameters": [
        {
          "in": "query",
          "name": "partNumber",
          "schema": {
            "type": "number"
          }
        },
        {
          "in": "query",
          "name": "uploadId",
          "schema": {
            "type": "number"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "OK"
        },
        "400": {
          "description": "Failed. Bad post data."
        }
      }
    },
    "post": {
      "summary": "Get upload URL",
      "tags": ["Project"],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "partNumber": {
                  "type": "number"
                },
                "uploadId": {
                  "type": "string"
                },
                "keyPath": {
                  "type": "string"
                }
              },
              "required": ["partNumber", "uploadId", "keyPath"]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "OK",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "presignedUrl": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "400": {
          "description": "Failed. Bad post data."
        },
        "500": {
          "description": "Internal Server Error"
        }
      }
    }
  },

  "/file_upload/start_upload": {
    "post": {
      "summary": "Start upload",
      "tags": ["Project"],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "projectName": {
                  "type": "string"
                },
                "fileName": {
                  "type": "string"
                },
                "modelId": {
                  "type": "string"
                },
                "atlasId": {
                  "type": "string"
                },
                "scviHubId": {
                  "type": "string"
                },
                "model_setup_anndata_args": {
                  "type": "object"
                },
                "classifierId": {
                  "type": "string"
                }
              },
              "required": ["fileName", "projectName"],
              "additionalProperties": false
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "OK"
        },
        "400": {
          "description": "Failed. Bad post data."
        }
      }
    }
  },
  "/file_upload/start_upload_for_atlas": {
    "post": {
      "summary": "Start upload for atlas",
      "tags": ["Atlas"],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "previewPictureURL": {
                  "type": "string"
                },
                "modalities": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "numberOfCells": {
                  "type": "number"
                },
                "species": {
                  "type": "string"
                },
                "compatibleModels": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "uploadedBy": {
                  "type": "string"
                },
                "atlasUrl": {
                  "type": "string"
                },
                "selectedClassifier": {
                  "type": "string"
                }
              },
              "required": [
                "name",
                "modalities",
                "numberOfCells",
                "species",
                "compatibleModels",
                "uploadedBy"
              ],
              "additionalProperties": false
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "OK",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "atlas": {
                    "$ref": "#/components/schemas/Atlas"
                  },
                  "models": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Model"
                    }
                  }
                }
              }
            }
          }
        },
        "400": {
          "description": "Failed. Bad post data."
        },
        "500": {
          "description": "Server error"
        }
      }
    }
  }
}
